	<dependency>
	  <groupId>org.springframework.boot</groupId>
	  <artifactId>spring-boot-dependencies</artifactId>
	  <version>1.5.7.RELEASE</version>
	  <type>pom</type>
	  <scope>import</scope>
	</dependency>
      </dependencies>
      </dependencyManagement>

</project>

